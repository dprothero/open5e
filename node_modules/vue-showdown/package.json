{
  "name": "vue-showdown",
  "version": "4.0.0",
  "description": "Use showdown as a vue component",
  "keywords": [
    "vue",
    "showdown"
  ],
  "homepage": "https://vue-showdown.js.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meteorlxy/vue-showdown.git"
  },
  "license": "MIT",
  "author": "meteorlxy <meteor.lxy@foxmail.com> (https://www.meteorlxy.cn)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/vue-showdown.esm.js",
      "require": "./dist/vue-showdown.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/vue-showdown.cjs.js",
  "unpkg": "./dist/vue-showdown.min.js",
  "module": "./dist/vue-showdown.esm.js",
  "browser": "./dist/vue-showdown.min.js",
  "types": "./dist/vue-showdown.d.ts",
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{json,yml,html}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "prettier": "@meteorlxy/prettier-config",
  "dependencies": {
    "@types/showdown": "^2.0.0",
    "showdown": "^2.1.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@meteorlxy/eslint-config-prettier": "^2.19.0",
    "@meteorlxy/eslint-config-prettier-typescript-vue": "^2.19.0",
    "@meteorlxy/prettier-config": "^2.11.0",
    "@meteorlxy/tsconfig": "^2.16.0",
    "@rollup/plugin-terser": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vuepress/client": "2.0.0-beta.59",
    "@vuepress/utils": "2.0.0-beta.59",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.7.4",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "sort-package-json": "^2.1.0",
    "typescript": "^4.9.4",
    "vite": "~4.0.1",
    "vuepress": "2.0.0-beta.59"
  },
  "packageManager": "pnpm@7.18.1",
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "build": "rimraf dist && rollup --config rollup.config.js && rimraf dist/types",
    "dev": "vite",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "lint": "eslint --ext .js,.ts,.vue .",
    "version": "pnpm lint && pnpm build && conventional-changelog -p angular -i CHANGELOG.md -s -r 1 && git add dist CHANGELOG.md"
  }
}